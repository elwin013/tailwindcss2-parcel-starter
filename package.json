{
  "name": "tailwindcss2-parcel-starter",
  "version": "1.0.0",
  "description": "Simple TailwindCSS starter with Parcel",
  "scripts": {
    "prebuild": "rimraf .cache && rimraf dist",
    "build": "parcel build src/index.html",
    "start": "parcel src/index.html",
    "tailwind": "npx tailwindcss build node_modules/tailwindcss/tailwind.css -o .tailwind/tailwind.css"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.8.6",
    "postcss": "7.0.35",
    "tailwindcss": "2.0.1-compat"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2"
  }
}
